procedure TStatistics_methods_Run_Result.Button3Click(Sender: TObject);
  var
    Name, Znach: string;
  begin
    //{->устанавливаем новые Дэфолтные значения изменяемых параметров
    // для 1-го параметра
    Name := Statistics_methods_Run_Result.Param_1.Caption;
    // берём из гуя название переменной
    Znach := znach_param_1_opt.Text;
    // берём из гуя новое значение переменной
    Statistics_methods_Main.UstanovkaParametra(Statistics_methods_Main,
        Name, Znach);  //устанавливаем параметр
    // для 1-го параметра

    // для 2-го параметра
    Name := Statistics_methods_Run_Result.Param_2.Caption;
    // берём из гуя название переменной
    Znach := znach_param_2_opt.Text;
    // берём из гуя новое значение переменной
    Statistics_methods_Main.UstanovkaParametra(Statistics_methods_Main,
        Name, Znach);  //устанавливаем параметр
    // для 2-го параметра

    // для 3-го параметра
    Name := Statistics_methods_Run_Result.Param_3.Caption;
    // берём из гуя название переменной
    Znach := znach_param_3_opt.Text;
    // берём из гуя новое значение переменной
    Statistics_methods_Main.UstanovkaParametra(Statistics_methods_Main,
        Name, Znach);  //устанавливаем параметр
    // для 1-го параметра

    // для 4-го параметра
    Name := Statistics_methods_Run_Result.Param_4.Caption;
    // берём из гуя название переменной
    Znach := znach_param_4_opt.Text;
    // берём из гуя новое значение переменной
    Statistics_methods_Main.UstanovkaParametra(Statistics_methods_Main,
        Name, Znach);  //устанавливаем параметр
    // для 4-го параметра
    //{<-устанавливаем новые Дэфолтные значения изменяемых параметров

        /// устанавливаем новые Дэфолтные значения допустимых напряжений.
        // для Материала 1.
        Name := 'ORT3';
        Znach := Dopust_napr_mater1.Text;
        Statistics_methods_Main.UstanovkaParametra(Statistics_methods_Main,
           Name, Znach);
        // для Материала 1.
        // для Материала 2.
        Name := 'ORT10';
        Znach := Dopust_napr_mater2.Text;
        Statistics_methods_Main.UstanovkaParametra(Statistics_methods_Main,
            Name, Znach);
        // для Материала 2.
        // для Материала 3.
        Name := 'ORT17';
        Znach := Dopust_napr_mater3.Text;
        Statistics_methods_Main.UstanovkaParametra(Statistics_methods_Main,
            Name, Znach);
        // для Материала 3.
        /// устанавливаем новые Дэфолтные значения допустимых напряжений.


    //{->Расчёт с  новыми Дэфолтными параметрами
    MainForm1.RunClick(Form3);
    WaitForm1.StartShow('Метод Статист. Испыт. применяет новые значения');
    repeat
        Application.ProcessMessages;
    until WaitForm1.FileExecuted = false;
    //{<-Расчёт с  новыми Дэфолтными параметрами

    //{закрываем окно результатов расчёта
    Statistics_methods_Run_Result.Close;
  end;



procedure TStatistics_methods_Run_Result.Button1Click(Sender: TObject);
  begin
    Statistics_methods_Run_Result.Close;
  end;

end.
